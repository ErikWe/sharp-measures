{
    "type": "Unit",
    "name": "Length",
    "quantity": "[name]",
    "units": [
        { "name": "Metre", "plural": "+s", "symbol": "m", "SI": true, "value": "1" },
        { "name": "Femtometre", "plural": "+s", "prefix": { "from": "Metre", "with": "Femto" } },
        { "name": "Picometre", "plural": "+s", "prefix": { "from": "Metre", "with": "Pico" } },
        { "name": "Nanometre", "plural": "+s", "prefix": { "from": "Metre", "with": "Nano" } },
        { "name": "Micrometre", "plural": "+s", "prefix": { "from": "Metre", "with": "Micro" } },
        { "name": "Millimetre", "plural": "+s", "prefix": { "from": "Metre", "with": "Milli" } },
        { "name": "Centimetre", "plural": "+s", "prefix": { "from": "Metre", "with": "Centi" } },
        { "name": "Decimetre", "plural": "+s", "prefix": { "from": "Metre", "with": "Deci" } },
        { "name": "Kilometre", "plural": "+s", "prefix": { "from": "Metre", "with": "Kilo" } },
        { "special": true, "separator": true },
        { "name": "AstronomicalUnit", "plural": "+s", "scaled": { "from": "Metre", "by": "1.495978797 * Math.Pow(10, 11)" } },
        { "name": "LightYear", "plural": "+s", "scaled": { "from": "Metre", "by": "9460730472580800" } },
        { "name": "Parsec", "plural": "+s", "scaled": { "from": "AstronomicalUnit", "by": "648000 / Math.PI" } },
        { "name": "Kiloparsec", "plural": "+s", "prefix": { "from": "Parsec", "with": "Kilo" } },
        { "name": "Megaparsec", "plural": "+s", "prefix": { "from": "Parsec", "with": "Mega" } },
        { "name": "Gigaparsec", "plural": "+s", "prefix": { "from": "Parsec", "with": "Giga" } },
        { "special": true, "separator": true },
        { "name": "Inch", "plural": "+es", "scaled": { "from": "Millimetre", "by": "25.4" } },
        { "name": "Foot", "plural": "Feet", "scaled": { "from": "Inch", "by": 12 } },
        { "name": "Yard", "plural": "+s", "scaled": { "from": "Foot", "by": 3 } },
        { "name": "Mile", "plural": "+s", "scaled": { "from": "Yard", "by": 1760 } }
    ]
}